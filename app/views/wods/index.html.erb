<h1>Listing wods</h1>

<table>
  <tr>
    <th>Work out</th>
    <th>Comments</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @wods.each do |wod| %>
  <tr>
    <td><%= wod.work_out %></td>
    <td><%= wod.comments %></td>
    <td><%= link_to 'Show', wod %></td>
    <td><%= link_to 'Edit', edit_wod_path(wod) %></td>
    <td><%= link_to 'Destroy', wod, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Wod', new_wod_path %>
